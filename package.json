{
  "name": "angular-demo-app-advanced-starter",
  "version": "1.0.0",
  "description": "Angular Demo Application to show some cool stuff :-)",
  "license": "MIT",
  "jest-junit": {
    "suiteName": "angular-demo-app",
    "output": "./tmp/junit.xml"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:all": "ng serve --proxy-config config/proxy.conf.json & npm run start:server",
    "start:server": "node server",
    "start:client": "ng serve --proxy-config config/proxy.conf.json --configuration hmr",
    "start:dist": "ng build --prod & DIST=true node server",
    "build": "ng build --prod",
    "build:report:es5": "ng build --stats-json && webpack-bundle-analyzer dist/stats-es5.json",
    "build:report:es2015": "ng build --stats-json && webpack-bundle-analyzer dist/stats-es2015.json",
    "jest": "jest --silent",
    "tdd": "jest --bail --expand --verbose",
    "tdd:cover": "jest --coverage --no-cache",
    "test": "ng test",
    "test:cover": "jest --silent --coverage --no-cache && open-cli coverage/index.html",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "check": "ncu",
    "transloco:extract": "transloco-keys-manager extract -l en de --sort -d missingValue",
    "transloco:optimize": "transloco-optimize dist/assets/i18n",
    "i18n:extract": "ng xi18n --output-path locale",
    "i18n:extract:xmb": "ng xi18n --output-path locale  --i18n-format=xmb",
    "ngx:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/de.json --clean --sort --format namespaced-json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/forms": "8.2.13",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/router": "8.2.13",
    "@ngneat/transloco": "2.3.1",
    "@ngneat/transloco-messageformat": "1.1.2",
    "@ngrx/effects": "8.4.0",
    "@ngrx/entity": "8.4.0",
    "@ngrx/router-store": "8.4.0",
    "@ngrx/store": "8.4.0",
    "@ngrx/store-devtools": "8.4.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "core-js": "2.6.5",
    "messageformat": "2.3.0",
    "ngrx-store-localstorage": "8.0.0",
    "ngx-translate-messageformat-compiler": "4.5.0",
    "rxjs": "6.5.3",
    "tslib": "1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.17",
    "@angular/cli": "8.3.17",
    "@angular/compiler-cli": "8.2.13",
    "@angular/language-service": "8.2.13",
    "@angularclass/hmr": "2.1.3",
    "@biesbjerg/ngx-translate-extract": "4.2.0",
    "@ngneat/transloco-keys-manager": "1.0.1",
    "@ngneat/transloco-optimize": "1.0.1",
    "@ngrx/schematics": "8.4.0",
    "@types/jasmine": "3.4.5",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.5",
    "body-parser": "1.19.0",
    "codelyzer": "5.2.0",
    "compression": "1.7.4",
    "express": "4.17.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.9.0",
    "jest-preset-angular": "6.0.2",
    "karma": "4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-mocha-reporter": "2.2.5",
    "material-design-icons": "3.0.1",
    "npm-check-updates": "^3.2.0",
    "open-cli": "5.0.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
